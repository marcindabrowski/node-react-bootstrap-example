{
  "name": "node-react-bootstrap-example",
  "private": true,
  "version": "0.9.0",
  "description": "Sample node.js application with ReactJS and Bootstrap 3.",
  "keywords": [
    "react",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcindabrowski/node-react-bootstrap-example.git"
  },
  "bugs": {
    "url": "https://github.com/marcindabrowski/node-react-bootstrap-example/issues"
  },
  "homepage": "https://github.com/marcindabrowski/node-react-bootstrap-example#readme",
  "author": "Marcin DÄ…browski",
  "license": "MIT",
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.3.0",
    "yarn": ">=0.27.5"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "Edge 15",
    "Safari 10.1"
  ],
  "scripts": {
    "test": "run-p test:*",
    "test:eslint": "eslint src webpack.config.babel.js --ext .js,.jsx --fix",
    "test:unit": "cross-env NODE_ENV=test nyc mocha test/.setup.js test/**/*.spec.js --recursive -r mock-local-storage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "run-s dev:*",
    "dev:clean": "rimraf public/assets/",
    "dev:webpack": "cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "run-s prod:*",
    "prod:clean": "rimraf public/assets/",
    "prod:webpack": "cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "start": "cross-env NODE_ENV=dev node server.hot.reload",
    "serve:nodemon": "nodemon ./src/server/index.js",
    "serve:node": "node ./src/server/index.js",
    "serve-dev": "run-s dev serve:nodemon",
    "serve-prod": "run-s prod serve:node",
    "release": "run-s test prod",
    "heroku-postbuild": "yarn prod"
  },
